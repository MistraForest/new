<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.getingstarted.pogba.helloworld.MainHelloActivity"
        android:background="#005544">


    <TextView
            android:text="@string/thedoualaboy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:textColor="#FFFFFF"
            android:layout_marginTop="41dp" android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>
    <Button
            android:text="@string/tapme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_below="@+id/textView"
            android:layout_alignRight="@+id/textView"
            android:layout_alignEnd="@+id/textView"
            android:layout_marginTop="26dp"
            android:id="@+id/tape_me"
    />
</RelativeLayout>
